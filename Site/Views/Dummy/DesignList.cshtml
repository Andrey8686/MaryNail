@using Site.Models
@using _Data.Models
@{
    var designs = (List<NailDesign>)ViewBag.Design;
    Layout = null;
}

@foreach (var item in designs)
{
    <div>
        <div class="nail-@item.Id in-list" onclick="Dummy.Add('@item.Id')">
            <img src="@Url.Action("Index", "Image", new ImageModel {Id = item.Id, AppDataDir = "NailDesign"})" alt="" />
            <p></p>
        </div>
    </div>
}